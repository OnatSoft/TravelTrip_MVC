
@{
    Layout = null;
}

<!DOCTYPE HTML>
<html>
<head>
    <title>Easy Trip Blog - Onat Somer</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" href="~/Themes/web/css/style.css" type="text/css" media="all" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link href="~/Themes/web/css/slider.css" rel="stylesheet" type="text/css" media="all" />
    <script src="~/Themes/web/js/jquery.min.js"></script>
    <script src="~/Themes/web/js/responsiveslides.min.js"></script>

    <script>
    $(function () {
      $("#slider1").responsiveSlides({
         auto: true,
		 nav: true,
		 speed: 500,
		 namespace: "callbacks",
      });
    });</script>
</head>
<body>

    <div class="header">
        <div class="wrap">
            <div class="logo"><a href="/anasayfa/"><img src="~/Themes/web/images/logo.png" alt="" /></a></div>
            <div class="con-right">
                <div class="grid1-l-img">
                    <img src="~/Themes/web/images/contact.png" alt="">
                </div>
                <div class="grid1-l-desc">
                    <p> Telefon: +91 00000-55555 </p>
                </div>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
        </div>
    </div>
    <div class="header-bottom">
        <div class="wrap">
            <ul id="nav">
                <li><a href="/anasayfa/"> Ana Sayfa </a></li>
                <li><a class="hsubs" href="/hakkimizda/Main/"> Hakkımızda </a></li>
                <li>
                    <a class="hsubs" href="#"> Mekanlar </a>
                    <ul class="subs">
                        <li><a href="#"> Oteller </a></li>
                        <li><a href="#"> Restaurantlar </a></li>
                        <li><a href="#"> Müzeler </a></li>
                    </ul>
                </li>
                <li><a class="hsubs" href="/blog/Index/"> Blog </a></li>
                <li><a href="/iletisim/Main/"> İletişim </a></li>
            </ul>
        </div>
    </div>
    
    <div class"content">
        @RenderBody()
    </div>

    <div class="footer-bottom">
        <div class="wrap">
            <div class="copy">
                <p> &copy; @DateTime.Now.Year Easy Trip. Tüm Hakları Saklıdır. | Design by W3Layouts </p>
            </div>
        </div>
    </div>

</body>
</html>