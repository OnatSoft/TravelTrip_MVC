@using TravelTrip_MVCProject.Models.Classes
@model List<BlogTBL>

<div class="padding">
    <h4> En Çok Okunan 10 Yazı </h4>

    <ul class="list list-pad">
        @{ int i = 0; }
        @foreach (var blog10 in Model)
        {
            <li class="list-bg"><span>@{i++;} @i.</span><a href="/blog/BlogDetail/@blog10.ID"> @blog10.Baslik </a></li>
        }
    </ul>

</div>